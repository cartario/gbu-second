st Schedule = () => {\n  const { request, loading, error, clearError } = useHttp();\n  const [docs, setDocs] = React.useState(null);\n\n  const getDocs = React.useCallback(async ()=>{\n    try {\n      const response = await request(`/api/docs`);\n      setDocs(response.filter((each)=>!each.afisha))\n    }\n    catch(err){}\n  },[request]);\n\n  React.useEffect(()=>{    \n    getDocs();\n    \n  }, [getDocs]);\n\n  if(!docs){\n    return (<h1>Loading...</h1>)\n  }\n\n  docs.sort((b,a)=>new Date(a.createdAt)-new Date(b.createdAt));\n\n  console.log(docs)\n\n  const adapterDocs = docs.map((doc)=>({\n    title:doc.title,\n    link:doc.url\n  }));\n\n  \n  \n  return (\n    <>\n      <Navbar/>\n      <Header title=\"Расписание\" />\n      <main className=\"schedule\">      \n        <ul className=\"schedule__list\">\n          {schedules &&\n            [...adapterDocs, ...schedules].map((doc) => (\n              <li key={doc.title} className=\"schedule__item\">\n                <a href={doc.link}>{doc.title}</a>\n              </li>\n            ))}\n        </ul>\n      </main>\n      \n    </>\n  );\n};\n\nexport default Schedule;\n",["433","434"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events-slider.jsx",[],["435","436"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/App.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/navbar.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/home.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/events.jsx",[],["437","438"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/data/studios-mock.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/detail-page.jsx",[],["439","440"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/footer.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/contacts.jsx",[],["441","442"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/event.jsx",[],["443","444"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/events-popup.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/about.jsx",[],["445","446"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/documents.jsx",[],["447","448"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/index.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/events-month.jsx",[],["449","450"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/header.jsx",["451"],"import React from 'react';\nimport {useHistory} from 'react-router-dom';\n\nexport default function ({title}) {\n  const history = useHistory();\n\n  const handleClick = ()=>{\n    history.goBack();\n  }\n\n  return (\n    <header className=\"title-header\">\n      <h1 className=\"title-top\"><span>{title}</span></h1>\n      <p className=\"title-goback\" onClick={handleClick}><span>Назад</span></p>      \n    </header>\n  );\n};\n\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/studioCard.jsx",["452"],"import React from 'react';\nimport {useHistory} from 'react-router-dom';\n\nexport default function ({_id: id, name, title, description, imgUrl, price}) {\n  const history = useHistory();\n\n  const handleClick = (id) => {\n    history.push(`/detail/${id}`);\n  }\n\n  return (\n    <article onClick = {()=>handleClick(id)} className=\"card\">\n      <div className=\"card__img\">\n        <img src={imgUrl? imgUrl : \"https://bayramix.ru/local/templates/bayramix_new/images/load.gif\"} width=\"300\" alt=\"cardImg\" />\n        <p>{name}</p>\n      </div>\n      <div className=\"card__text\">\n        <p className=\"card__description\">\n          {description ? description.substr(0, 180) : \"Description\"}...\n        </p>\n        <p className=\"card__price\">{price === 'free'? \"бесплатно\": price}</p>\n      </div>\n    </article>\n  );\n}\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/welcome.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/sessions-today-board.jsx",["453"],"import React from 'react';\nimport SessionTodayAddress from '../components/sessions-today-addres';\n// import { studios } from '../data/studios-mock';\nimport { sayWelcome } from '../utils';\n\nconst HOLIDAY = 0;\nconst day = (new Date()).getDay();\n\nconst SessionsTodayBoard = ({studios}) => {\n  const [todayCards, setTodayCards] = React.useState(null);\n  const dayStatus = sayWelcome();\n\n  React.useEffect(() => {\n    //TODO fetch cards\n    const todayCards = studios.filter((card) => {\n      if (!card.day) {\n        return null;\n      }\n      return card.day.includes(day);\n    });\n\n    setTodayCards(todayCards);\n  }, []);\n\n  if (!todayCards) {\n    return null;\n  }\n\n  const addreses = [...new Set(todayCards.map((card) => card.adress).sort(() => -1))];\n  //todo replace to reduce, get Lusin of the list by first priority\n\n  return (\n    <section className=\"now\">\n      <div>\n        {day === HOLIDAY ? (\n          <h3 className=\"now__holiday\">\n            Сегодня выходной и занятия закончилиь. Однако до понедельника вы можете присмотреть себе{' '}\n            <a href=\"/studios\">студию/секцию</a> по душе и записаться онлайн\n          </h3>\n        ) : (\n          ''\n        )}\n      </div>\n      {todayCards.length && dayStatus.status !== 0 ? (\n        <div className=\"sessions\">\n          <h3>Занятия на сегодня:</h3>\n          {addreses.map((addres) => (\n            <SessionTodayAddress\n              key={addres}\n              dataList={todayCards.filter((card) => card.adress === addres)}              \n            />\n          ))}\n        </div>\n      ) : (\n        ''\n      )}\n    </section>\n  );\n};\n\nexport default SessionsTodayBoard;\n",["454","455"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/utils.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events-categories.jsx",[],["456","4