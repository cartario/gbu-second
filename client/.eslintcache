ditMode) {\n      request(`api/docs/${form._id}`, 'PATCH', form);\n      window.location.reload(); //temporary\n    }\n  };\n\n  const handleToggleForm = () => {\n    setVisible(!visible);\n  };\n\n  return (\n    <li className=\"admin-item\">\n      <div className=\"admin-item__top\">\n        <p className=\"admin-item__title\" onClick={handleToggleForm}>\n          <span>{title}</span>\n        </p>\n\n        {visible && (\n          <form onSubmit={(e) => e.preventDefault()} className=\"admin-item__form\">\n            <div className=\"admin-item__field\">\n              <label>\n                Title\n                <input\n                  name=\"title\"\n                  disabled={!editMode}\n                  type=\"text\"\n                  value={form.title}\n                  onChange={handleClickForm}\n                />\n              </label>\n            </div>\n            <div className=\"admin-item__field\">\n              <label>\n                Url\n                <input\n                  name=\"url\"\n                  disabled={!editMode}\n                  type=\"text\"\n                  value={form.url}\n                  onChange={handleClickForm}\n                />\n              </label>\n            </div>\n\n            <div className=\"admin-item__field\">\n              <label>\n                Afisha\n                <input\n                  disabled={!editMode}\n                  type=\"checkbox\"\n                  name=\"afisha\"\n                  checked={form.afisha}\n                  onChange={handleClickForm}\n                />\n              </label>\n            </div>\n\n            <div className=\"admin-item__controls\">\n              <button className=\"admin-item__controls--edit\" onClick={handleEditMode}>\n                {editMode ? 'Save' : 'Edit'}\n              </button>\n              {editMode ? (\n                <>\n                  <button\n                    className=\"admin-item__controls--cancel\"\n                    onClick={() => setEditMode(false)}\n                  >\n                    Cancel\n                  </button>\n                  <button\n                    className=\"admin-item__controls--delete\"\n                    onClick={() => {\n                      handleDeleteEvent(doc._id);\n                    }}\n                  >\n                    Delete\n                  </button>\n                </>\n              ) : (\n                ''\n              )}\n            </div>\n          </form>\n        )}\n      </div>\n    </li>\n  );\n};\n\n// const docs = [\n//   {\n//     _id: Date.now().toString(),\n//     title: 'test title',\n//     url: 'test url',\n//   },\n// ];\n\nconst ScheduleDocs = () => {\n  const { request, loading, error, clearError } = useHttp();\n  const [showSection, setShowSection] = React.useState(false);\n  const [visible, setVisible] = React.useState(false);\n  const [docs, setDocs] = React.useState(null);\n\n  const [form, setForm] = React.useState({\n    title: '',\n    url: '',\n    afisha: false,\n  });\n\n  const handleClickShowSection = () => {\n    setShowSection(!showSection);\n  };\n\n  const getDocs = React.useCallback(async () => {\n    try {\n      const response = await request(`/api/docs`);\n      setDocs(response.filter((each)=>each.afisha));\n    } catch (err) {}\n  }, [request]);\n\n  React.useEffect(() => {\n    getDocs();\n  }, [getDocs]);\n\n  const handleClickForm = (e) => {\n    const target = e.target.value;\n    const name = e.target.name;\n\n    setForm((prev) => ({\n      ...form,\n      [name]: target,\n      afisha: e.target.checked,\n    }));\n  };\n\n  const handleSubmit = async () => {\n    try {\n      const response = await request('/api/docs/create', 'POST', form);\n      //  setEvent(response);\n      window.location.reload(); //temporary\n    } catch (err) {}\n  };\n\n  if (!docs) {\n    return <h1>Loading...</h1>;\n  }\n\n  return (\n    <div className=\"admin-section\">\n      <h2 onClick={handleClickShowSection}>\n        Афиши {showSection ? '(свернуть)' : '(развернуть)'}\n      </h2>\n\n      {showSection && (\n        <>\n          {!visible && (\n            <button className=\"admin-section__button\" onClick={() => setVisible(true)}>\n              +\n            </button>\n          )}\n          {visible && (\n            <div className=\"admin-item admin-item--new\">\n              <p onClick={() => setVisible(false)} className=\"admin-item__title\">\n                Новое расписание\n              </p>\n              <form className=\"admin-item__form\" onSubmit={(e) => e.preventDefault()}>\n                <div className=\"admin-item__field\">\n                  <label>\n                    Title\n                    <input\n                      type=\"text\"\n                      name=\"title\"\n                      value={form.title}\n                      onChange={handleClickForm}\n                      required\n                    />\n                  </label>\n                </div>\n                <div className=\"admin-item__field\">\n                  <label>\n                    Url\n                    <input\n                      type=\"text\"\n                      name=\"url\"\n                      value={form.url}\n                      onChange={handleClickForm}\n                      required\n                    />\n                  </label>\n                </div>\n\n                <div className=\"admin-item__field\">\n                  <label>\n                    Afisha\n                    <input\n                      type=\"checkbox\"\n                      name=\"afisha\"\n                      checked={form.afisha}\n                      onChange={handleClickForm}\n                    />\n                  </label>\n                </div>\n                <button\n                  onClick={() => {\n                    handleSubmit();\n                  }}\n                >\n                  Submit\n                </button>\n              </form>\n            </div>\n          )}\n          <ul>\n            {docs ? docs.map((doc) => <AdminDocCard key={doc._id} doc={doc} />) : <p>Loading...</p>}\n          </ul>\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default ScheduleDocs;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/page-join.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/modal.jsx",[],["490","491"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/redux/mainPostReducer.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/index.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin/MainPost.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/select.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/index.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/toggleVisiblePost.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin/UploadPoster.js",["492"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/backdrop.js",["493"],"import React from 'react';\nimport Backdrop from '@material-ui/core/Backdrop';\nimport CircularProgress from '@material-ui/core/CircularProgress';\nimport Button from '@material-ui/core/Button';\nimport { makeStyles } from '@material-ui/core/styles';\n\nconst useStyles = makeStyles((theme) => ({\n  backdrop: {\n    zIndex: theme.zIndex.drawer + 1,\n    color: '#fff',\n  },\n}));\n\nexport default function SimpleBackdrop() {\n  const classes = useStyles();\n  const [open, setOpen] = React.useState(true);\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n     \n      <Backdrop className={classes.backdrop} open={open} onClick={handleClose}>\n        <CircularProgress color=\"inherit\" />\n      </Backdrop>\n    </div>\n  );\n}","/Users/vasiliy/Desktop/academy/gbu-second/