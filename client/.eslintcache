Mode && (\n          <>\n            <button style={{ backgroundColor: 'lightgreen' }} onClick={handleUpdate}>\n              Сохранить изменения\n            </button>\n\n            <button onClick={() => setEditMode(false)} onClick={handleCancel}>\n              Отменить\n            </button>\n          </>\n        )}\n      </div>\n    </div>\n  );\n};\n\nconst AdminContactPageSocial = ({ social }) => {\n  return (\n    <ul>\n      {social.map((item)=><AdminContactPageSocialItem key={item.id} socialItem={item}/>)}\n    </ul>\n  );\n};\n\nconst AdminContactPageSocialItem = ({ socialItem }) => {\n  const [form, setForm] = React.useState(socialItem);\n  const [editMode, setEditMode] = React.useState(false);\n  const [isSuccesfullSent, setSuccesfullSent] = React.useState(null);\n\n  const handleChange = (e) => {\n    const name = e.target.name;\n    const value = e.target.value;\n    setForm({ ...form, [name]: value });\n  };\n\n  const handleCancel = () => {\n    if (window.confirm('Выйти без изменений?')) {\n      setForm(socialItem);\n      setEditMode(false);\n    }\n  };\n\n  const handleUpdate = () => {\n    if (window.confirm('Уверены что хотите обновить?')) {\n      console.log('updating');\n\n      const updateData = async () => {\n        try {\n          const res = await fetch(`${BASE_URL}/contactPage/items/social/${socialItem.id}.json`, {\n            method: 'PATCH',\n            headers: {\n              'Content-Type': 'application/json',\n            },\n            body: JSON.stringify(form),\n          });\n\n          if(res){\n            setSuccesfullSent(true);\n            setEditMode(false);\n            window.location.reload();\n          }\n        } catch (err) {\n          throw err;\n        }\n      };\n\n      updateData();\n    }\n  };\n\n  React.useEffect(()=>{\n    let timer;\n\n    if(isSuccesfullSent){\n      timer = setTimeout(()=>{\n        setSuccesfullSent(null);\n      }, 3000)\n    }\n\n    return ()=>{\n      clearTimeout(timer);\n\n    }\n  },[isSuccesfullSent])\n\n  return (\n    <div>    \n\n      {Object.keys(socialItem).filter((each)=>each!=='id').map((field) => (\n        <div key={field} className=\"admin-item__field\">\n          <label>\n            {field}\n            <input\n              disabled={!editMode}\n              name={field}\n              type=\"text\"\n              value={form[field]}\n              onChange={handleChange}\n            />\n          </label>\n        </div>\n      ))}\n\n      <div>\n      {isSuccesfullSent && <p style={{backgroundColor: 'green', padding: '10px'}}>Успешно отправлено</p>}\n        {!editMode && (\n          <button style={{ marginBottom: '20px' }} onClick={() => setEditMode(true)}>\n            Edit\n          </button>\n        )}\n        {editMode && (\n          <>\n\n            <button style={{ backgroundColor: 'lightgreen' }} onClick={handleUpdate}>\n              Сохранить изменения\n            </button>\n\n            <button onClick={() => setEditMode(false)} onClick={handleCancel}>\n              Отменить\n            </button>\n          </>\n        )}\n      </div>\n    </div>\n  );\n};\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-section.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-add-button.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-sum-page.jsx",["304","305","306","307"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/afisha.jsx",["308","309","310"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-page1.jsx",["311","312"],{"ruleId":"313","severity":1,"message":"314","line":13,"column":19,"nodeType":"315","messageId":"316","endLine":13,"endColumn":26},{"ruleId":"317","replacedBy":"318"},{"ruleId":"319","replacedBy":"320"},{"ruleId":"313","severity":1,"message":"314","line":65,"column":20,"nodeType":"315","messageId":"316","endLine":65,"endColumn":27},{"ruleId":"313","severi