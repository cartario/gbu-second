ents/sessions-today-addres';\n// import { studios } from '../data/studios-mock';\nimport { sayWelcome } from '../utils';\n\nconst HOLIDAY = 0;\nconst day = (new Date()).getDay();\n\nconst SessionsTodayBoard = ({studios}) => {\n  const [todayCards, setTodayCards] = React.useState(null);\n  const dayStatus = sayWelcome();\n\n  React.useEffect(() => {\n    //TODO fetch cards\n    const todayCards = studios.filter((card) => {\n      if (!card.day) {\n        return null;\n      }\n      return card.day.includes(day);\n    });\n\n    setTodayCards(todayCards);\n  }, []);\n\n  if (!todayCards) {\n    return null;\n  }\n\n  const addreses = [...new Set(todayCards.map((card) => card.adress).sort(() => -1))];\n  //todo replace to reduce, get Lusin of the list by first priority\n\n  return (\n    <section className=\"now\">\n      <div>\n        {day === HOLIDAY ? (\n          <h3 className=\"now__holiday\">\n            Сегодня выходной и занятия закончилиь. Однако до понедельника вы можете присмотреть себе{' '}\n            <a href=\"/studios\">студию/секцию</a> по душе и записаться онлайн\n          </h3>\n        ) : (\n          ''\n        )}\n      </div>\n      {todayCards.length && dayStatus.status !== 0 ? (\n        <div className=\"sessions\">\n          <h3>Занятия на сегодня:</h3>\n          {addreses.map((addres) => (\n            <SessionTodayAddress\n              key={addres}\n              dataList={todayCards.filter((card) => card.adress === addres)}              \n            />\n          ))}\n        </div>\n      ) : (\n        ''\n      )}\n    </section>\n  );\n};\n\nexport default SessionsTodayBoard;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/utils.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events-categories.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/sessions-today-addres.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/studios-compilation.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/hooks/http.hook.js",["400"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/context/auth.context.js",[],["401","402"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/hooks/auth.hook.js",[],["403","404"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/constants.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/total-info-block.jsx",["405","406","407"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/say-hello.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-event-card.jsx",["408"],"import React from 'react';\nimport useHttp from '../hooks/http.hook';\nimport FileInputComponent from '../components/FileInput';\n\nconst AdminEventCard = ({ event, setEvents }) => {\n  const [visible, setVisible] = React.useState(false);\n  const [editMode, setEditMode] = React.useState(false);\n  const [form, setForm] = React.useState(event);\n  const { request } = useHttp();\n\n  const onFileInputChange = React.useCallback((url) => {\n    setForm({ ...form, posterUrl: url });\n  }, []);\n\n  const handleClickForm = (e) => {\n    const target = e.target.value;\n    const name = e.target.name;\n\n    setForm({\n      ...form,\n      [name]: target,\n    });\n  };\n\n  const handleEditMode = () => {\n    setEditMode(!editMode);\n    handleUpdateEvent(form);\n  };\n\n  const handleDeleteEvent = (id) => {\n    if(window.confirm('Вы действительно хотите удалить событие?')){\n      request(`api/events/${id}`, 'DELETE');\n      window.location.reload(); //temporary\n    }\n  };\n\n  const handleUpdateEvent = (form) => {\n    if (editMode) {\n      if(window.confirm('Вы действительно хотите обновить событие?')){\n        request(`api/events/${form._id}`, 'PATCH', form);\n        window.location.reload(); //temporary\n      }\n    }\n  };\n\n  const handleToggleForm = () => {\n    setVisible(!visible);\n  };\n\n  return (\n    <li className=\"admin-item\">\n      <div className=\"admin-item__top\">\n        <p className=\"admin-item__title\" onClick={handleToggleForm}>\n          <span>{event.title}</span>\n          <span> -&nbsp;{event.date.toLocaleString()}</span>\n          <span> &nbsp; id: {event._id}</span>\n        </p>\n      </div>\n\n      {visible ? (\n        <form className=\"admin-item__form\" onSubmit={(e) => e.preventDefault()}>\n\n          <div className=\"admin-item__field\">\n            {/* <label>\n            PosterImage:\n            <input name=\"posterUrl\" type=\"text\" value={form.posterUrl} onChange={handleClickForm}/>\n            \n          </label> */}\n\n            <FileInputComponent handler={onFileInputChange} initialImgUrl={form.posterUrl} disabled={!editMode}/>\n          </div>\n\n          <div className=\"admin-item__field\">\n            <label>\n              Title:\n              <input\n                name=\"title\"\n                disabled={!editMode}\n                type=\"text\"\n                value={form.title}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div>\n\n          <div className=\"admin-item__field\">\n            <label>\n              Date:{form.date.toLocaleString()}\n              <input\n                name=\"date\"\n                disabled={!editMode}\n                type=\"datetime-local\"\n                value={form.date.toLocaleString()}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div>\n\n          <div className=\"admin-item__field\">\n            <label>\n              Category:\n              <input\n                name=\"category\"\n                disabled={!editMode}\n                type=\"text\"\n                value={form.category}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div>\n\n          <div className=\"admin-item__field\">\n            <label>\n              Place:\n              <input\n                name=\"place\"\n                disabled={!editMode}\n                type=\"text\"\n                value={form.place}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div>\n\n          <div className=\"admin-item__field\">\n            <label>\n              Description:\n              <textarea\n                rows={5}\n                name=\"description\"\n                disabled={!editMode}\n                value={form.description}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div>\n\n          <div className=\"admin-item__field\">\n          <label>\n            Photos:\n            <input\n              name=\"photos\"\n              type=\"text\"\n              value={form.photos}\n              onChange={handleClickForm}\n            />\n          </label>\n        </div>\n\n          {/* <div className=\"admin-item__field\">\n            <label>\n              PosterImage:\n              <input\n                name=\"posterUrl\"\n                disabled={!editMode}\n                type=\"text\"\n                value={form.posterUrl}\n                onChange={handleClickForm}\n              />\n            </label>\n          </div> */}\n\n          <div className=\"admin-item__img\">\n            <img style={{ width: '100px' }} src={event.posterUrl} alt=\"eventPoster\" />\n          </div>\n\n          <div className=\"admin-item__controls\">\n            <button className=\"admin-item__controls--edit\" onClick={handleEditMode}>\n              {editMode ? 'Save' : 'Edit'}\n            </button>\n            {editMode ? (\n              <>\n                <button className=\"admin-item__controls--cancel\" onClick={() => setEditMode(false)}>\n                  Cancel\n                </button>\n                <button\n                  className=\"admin-item__controls--delete\"\n                  onClick={() => {\n                    handleDeleteEvent(event._id);\n                  }}\n                >\n                  Delete\n                </button>\n              </>\n            ) : (\n              ''\n            )}\n          </div>\n        </form>\n      ) : (\n        ''\n      )}\n    </li>\n  );\n};\n\nexport default AdminEventCard;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-event-newcard.jsx",["409","410"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-studio.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-studio-newcard.jsx",["411"],"import React from 'react';\nimport useHttp from '../hooks/http.hook';\n\nconst AdminStudioNewCard = ({ handleShowNewStudio }) => {\n  const { request } = useHttp();\n\n  const [form, setForm] = React.useState({\n    name: '',\n    title: '',\n    groupNumber: '',\n    adress: '',\n    age_min: '',\n    day: '',\n    timeFrom: '',\n    timeTo: '',\n    price: '',\n    type: '',\n    isDuplicate: '',\n    age_max: '',\n    cab: '',\n    teacher: '',\n    priceOptions: '',\n    imgUrl: '',\n    description: '',\n    status: '',\n  });\n\n  const [studio, setStudio] = React.useState(null);\n\n  const handleClickForm = (e) => {\n    const target = e.target.value;\n    const name = e.target.name;\n\n    setForm({\n      ...form,\n      [name]: target,\n    });\n  };\n\n  const handleSubmit = async () => {\n   if(window.confirm('Вы действительно хотите добавить новую студию?')){\n    try {\n      const response = await request('/api/studios/create', 'POST', form);\n      setStudio(response);\n      window.location.reload(); //temporary\n    } catch (err) {}\n   }\n  };\n\n  return (\n    <div className=\"admin-item admin-item--new\">\n      <p\n        className=\"admin-item__title\"\n        onClick={() => {\n          handleShowNewStudio(false);\n        }}\n      >\n        New studio\n      </p>\n\n      <form className=\"admin-item__form\">\n        <div className=\"admin-item__field\">\n          <label>\n            *Название группы студий/секций\n            <input name=\"name\" type=\"text\" value={form.name} onChange={handleClickForm} required />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Название студии/секции\n            <input\n              name=\"title\"\n              type=\"text\"\n              value={form.title}\n              onChange={handleClickForm}\n              required\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Номер группы студии/секции:\n            <input\n              name=\"groupNumber\"\n              type=\"text\"\n              value={form.groupNumber}\n              onChange={handleClickForm}\n              required\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Место проведения занятий:\n            <input\n              name=\"adress\"\n              type=\"text\"\n              value={form.adress}\n              onChange={handleClickForm}\n              required\n              placeholder='трофимова или люсиновская'\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Минимальный возраст:\n            <input\n              name=\"age_min\"\n              type=\"text\"\n              value={form.age_min}\n              onChange={handleClickForm}\n              required\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *День недели:\n            <input name=\"day\" type=\"text\" value={form.day} onChange={handleClickForm} required \n              placeholder='пн-1, вт-2, ср-3 и тд'\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Стоимость:\n            <input\n              name=\"price\"\n              type=\"text\"\n              value={form.price}\n              onChange={handleClickForm}\n              required\n              placeholder='если бесплатно, то писать free'\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Время начала занятий:\n            <input\n              name=\"timeFrom\"\n              type=\"text\"\n              value={form.timeFrom}\n              onChange={handleClickForm}\n              required\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Время окончания занятий:\n            <input\n              name=\"timeTo\"\n              type=\"text\"\n              value={form.timeTo}\n              onChange={handleClickForm}\n              required\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            *Категория:\n            <input name=\"type\" type=\"text\" value={form.type} onChange={handleClickForm} required />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            isDuplicate:\n            <input\n              name=\"isDuplicate\"\n              type=\"text\"\n              value={form.isDuplicate}\n              onChange={handleClickForm}\n              placeholder='ставится 1 если студия уже была записана, чтобы избежать дублей'\n            />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            age_max:\n            <input name=\"age_max\" type=\"text\" value={form.age_max} onChange={handleClickForm} />\n          </label>\n        </div>\n\n        <div className=\"admin-item__field\">\n          <label>\n            cab:\n            <input