k={handlePosterClick}\n      />\n      {loading && <CircularProgress/>}      \n      <p>{status}</p>      \n    </>)\n};\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/backdrop.js",["518"],"import React from 'react';\nimport Backdrop from '@material-ui/core/Backdrop';\nimport CircularProgress from '@material-ui/core/CircularProgress';\nimport Button from '@material-ui/core/Button';\nimport { makeStyles } from '@material-ui/core/styles';\n\nconst useStyles = makeStyles((theme) => ({\n  backdrop: {\n    zIndex: theme.zIndex.drawer + 1,\n    color: '#fff',\n  },\n}));\n\nexport default function SimpleBackdrop() {\n  const classes = useStyles();\n  const [open, setOpen] = React.useState(true);\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n     \n      <Backdrop className={classes.backdrop} open={open} onClick={handleClose}>\n        <CircularProgress color=\"inherit\" />\n      </Backdrop>\n    </div>\n  );\n}","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/circularProgress.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/hooks/custom.hook.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/hooks/uploadFile.hook.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/Toast.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/Button.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/theme.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/Modal.js",["519"],"import React from 'react';\nimport PropTypes from 'prop-types';\nimport Button from '@material-ui/core/Button';\nimport Dialog from '@material-ui/core/Dialog';\n\nfunction SimpleDialog(props) {  \n  const { onClose, selectedValue, open, data } = props;\n\n  const handleClose = () => {\n    onClose(selectedValue);\n  };\n\n  return (\n    <Dialog onClose={handleClose} aria-labelledby=\"simple-dialog-title\" open={open}>\n      <img src={data} width='100%' height='100%'/>\n    </Dialog>\n  );\n}\n\nSimpleDialog.propTypes = {\n  onClose: PropTypes.func.isRequired,\n  open: PropTypes.bool.isRequired,\n  selectedValue: PropTypes.string.isRequired,\n};\n\nexport default function SimpleDialogDemo({data}) {\n  const [open, setOpen] = React.useState(false);  \n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = (value) => {\n    setOpen(false);    \n  };\n\n  return (\n    <div>      \n      <br />\n      <Button variant=\"outlined\" color=\"primary\" onClick={handleClickOpen}>\n        Open\n      </Button>\n\n      <SimpleDialog open={open} onClose={handleClose} data={data}/>\n    </div>\n  );\n}\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/Input.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/mui/DateTimePicker.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin/ColorPicker.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/main-poster-new.jsx",["520","521"],"import React from 'react';\nimport useHttp from '../hooks/http.hook';\nimport Logo from '../components/Logo';\nimport hexToHsl from 'hex-to-hsl';\n\nconst whatsAppPhone = '79032530773';\nconst URL = 'https://centerdaniil-b74b6-default-rtdb.firebaseio.com/adminPage/mainpost'\n\nconst Poster = () => {\n  const [data, setData] = React.useState(null);\n  const { request } = useHttp();\n  const [whatsApp, setWhatsApp] = React.useState('');\n\n  const handleWhatsApp = (e) => {\n    setWhatsApp(e.target.value);\n  };\n\n  React.useEffect(()=>{\n    (async function fetch (){\n      const response = await request(`${URL}.json`);      \n      setData(Object.values(response)[0]);\n    })()\n  }, [])\n\n  if(!data){\n    return null;\n  }  \n\n  const animated = false;\n  const { visible, date, title, address, age_category, posterUrl, colorContact, website,\n    phone, addressContact, social} = data;\n\n  if(!visible){\n    return null;\n  }\n\n  const hslArray = hexToHsl(colorContact);\n  const darken = `hsl(${hslArray[0]},${hslArray[1]}%,${hslArray[2] - 15}%)`;\n  const lighten = `hsl(${hslArray[0]},${hslArray[1]}%,${hslArray[2] + 15}%)`;\n  const contrastColor = `hsl(${hslArray[0] - 180},${hslArray[1]}%,${hslArray[2]}%)`;\n\n  return (\n    <div className=\"wrapper-poster\">\n      <h1>СОВСЕМ СКОРО:</h1>\n      <div className=\"poster\" style={{ backgroundImage: `url(${posterUrl})` }}>\n        <div className=\"poster-top\">          \n          <Logo opacity={0.7} color='#fff'/>          \n          <div className=\"container-t\">\n            <div\n              className={`poster-top__contacts ${animated ? 'freeze-rotate' : ''}`}\n              style={{\n                background: `linear-gradient(89.93deg, \t${colorContact} 0.06%, ${darken} 52.77%`,                \n              }}\n            >\n              <ul>\n                <li>{website}</li>\n                <li>{phone}</li>\n              </ul>\n              <ul>\n                <li>{addressContact}</li>\n                <li>{social}</li>\n              </ul>\n            </div>\n          </div>\n        </div>\n\n        <div className=\"poster-content\">\n          <div className=\"poster-content__title\">\n            <div className=\"container-t\">\n              <div className={`transform ${animated ? 'translate3d' : ''}`}>                \n                <h1>{title}</h1>\n                <p>{age_category}</p>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"poster-content__date a_left-right\">\n            <div className=\"container-t\">\n              <div className={`transform ${animated ? 'rotateY' : ''}`}>\n                <div style={{ display: 'flex' }}>\n                  <div\n                    style={{ color: lighten }}\n                    className=\"poster-content__date--date\"\n                  >\n                    {new Date(date).toLocaleDateString()}\n                    <br />\n                    <p style={{ color: '#fff' }}>\n                      {new Date(date).toLocaleTimeString().split(':').slice(0,2).join(':')}\n                    </p>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        {address.length &&<>\n        <div className=\"poster-divider\"></div>\n        <div className=\"poster-description\"\n        style={{          \n          backgroundColor: colorContact,\n          color: contrastColor\n        }}\n        >\n          <p>МЕСТО ПРОВЕДЕНИЯ: {address}</p>\n        </div></>\n        }\n\n        <div \n        style={{\n          backgroundColor: contrastColor\n        }}\n        className=\"poster-bottom\">\n          <h2>Отправить заявку на WhatsApp:</h2>\n          <div className=\"poster-bottom__whatsapp\">\n            <input\n              className=\"poster-bottom__whatsapp--input\"\n              placeholder=\"Введите ФИО\"\n              onChange={handleWhatsApp}              \n              value={whatsApp}\n            />\n\n            <a\n              className=\"poster-bottom__whatsapp--btn shake-btn\"\n              href={`https://api.whatsapp.com/send/?phone=%2B${whatsAppPhone}&text=${\n                'Запись на мероприятие:' + title + '__' + whatsApp\n              }&app_absent=0`}\n            ></a>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Poster;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/charts/SimpleLine.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/Logo.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/hack/Grid.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/redux/index.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/hack/Menu.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/hack/screens.js",[],"/Users/v