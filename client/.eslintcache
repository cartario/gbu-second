};\n\n  const handleShowNewStudio = (value) => {\n    setshowNewStudio(value);\n  };\n\n  const getStudios = useCallback(async ()=>{\n    try {\n      const response = await request(`/api/studios`);\n      setStudios(response)\n    }\n    catch(err){}\n  },[request]);\n\n  const getEvents = useCallback(async () => {\n    try {\n      const response = await request(`/api/events`);\n      setEvents(\n        response.map((event) => {\n          const date = new Date(event.date);\n          return { ...event, date };\n        }),\n      );\n    } catch (err) {}\n  }, [request]);\n\n  React.useEffect(()=>{    \n    getStudios();\n    getEvents();\n  }, [getStudios, getEvents]);\n\n  if(!studios){\n    return (<h1>Loading...</h1>)\n  }\n\n  const namesOfGroup = [... new Set(studios.map((studio)=>studio.name))];\n  const studiosBynamesOfGroup = namesOfGroup.map((group)=>studios.filter((studio)=>studio.name===group));\n\n  const StudiosByGroup = ({studios}) => {\n    return (\n    <ul>\n      {studios.map((studio) => <AdminStudioCard key={studio._id} studio={studio} />)}\n    </ul>)\n  }\n\n  return (\n    <>\n      <h1>AdminPage</h1>\n      <nav>\n        <ul>\n          <li>\n            <a href=\"/\">На главную</a>\n          </li>\n          <li>\n            {' '}\n            <a href=\"/admin\" onClick={() => auth.logout()}>\n              Logout\n            </a>\n          </li>\n        </ul>\n      </nav>\n\n      <ScheduleDocs />\n\n      <div>\n        <h2>Events</h2>\n\n        {showNewEvent ? (\n          <AdminEventNewCard handleShowNewEvent={handleShowNewEvent} />\n        ) : (\n          <button onClick={() => handleShowNewEvent(true)}>+</button>\n        )}\n        <ul>{events ?events.map((event) => <AdminEventCard setEvents={setEvents} key={event._id} event={event} />) : <p>Loading...</p>}</ul>\n      </div>\n\n      <AdminMainPost />\n\n      <div>\n        <h2>Studios</h2>\n\n        {showNewStudio ? (\n          <AdminStudioNewCard handleShowNewStudio={handleShowNewStudio} />\n        ) : (\n          <button onClick={() => handleShowNewStudio(true)}>+</button>\n        )}\n        {/* <ul>{studios ? studios.map((studio) => <AdminStudioCard key={studio._id} studio={studio} />) : <p>Loading...</p>}</ul> */}\n\n          {studiosBynamesOfGroup.map((studios, index)=>\n          <div key={index} className=\"admin-groups\">\n            <p>{index+1} Группа студий: {studios[0].name}</p>\n            <StudiosByGroup studios={studios}/>\n          </div>)}\n        \n      </div>      \n    </>\n  );\n};\n\nexport default AdminPage;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/minors.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/videoplayer.jsx",["224"],"import React from 'react';\nimport useHttp from '../hooks/http.hook';\n\nconst VideoPlayer = () => {\n  const [mainPost, setMainPost] = React.useState(null);\n  const {request} = useHttp();\n\n  const getMainPost = React.useCallback(async () => {\n    try {\n      const response = await request(`/api/mainpost`);\n      setMainPost(response);\n    } catch (err) {}\n  }, [request]);\n\n  React.useEffect(() => {\n    getMainPost();\n  }, [getMainPost]);\n \n  if(!mainPost){\n    return null;\n  } \n\n  return (\n    <>\n      {mainPost[0].videoplayer_visible ? \n      <div className=\"videoplayer\">\n      <h3>{mainPost[0].videoplayer_title}</h3>\n      <iframe        \n        // width=\"560\"\n        // height=\"315\"\n        src={mainPost[0].videoplayer_url}\n        frameBorder=\"0\"\n        allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n        allowFullScreen\n      ></iframe>\n    </div> : \"\"}\n    </>\n  );\n};\n\nexport default VideoPlayer;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/admin-main-post.jsx",["225"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/main-poster.jsx",["226"],"/Users/vasiliy/Desktop/academy/gbu-s