);\n      setStudios(response)\n    }\n    catch(err){}\n  },[request]);  \n\n  React.useEffect(()=>{    \n    getStudios();\n  }, [getStudios])\n  \n  if(!studios){\n    return (<>\n    <Navbar/>    \n      <Header title=\"Студии и секции\"/>\n    <h1>...Loading...</h1>\n    </>);\n  }\n\n  const studiosCopy = [...studios].filter((studio)=>!studio.isDuplicate);\n  const studiosKids = [...studios].filter((studio)=>!studio.isDuplicate&&studio.age_min<=6);\n  const studiosDance = [...studios].filter((studio)=>(!studio.isDuplicate&&(studio.type==='dance'))||(studio._id==='15'));\n  const studiosArt = [...studios].filter((studio)=>!studio.isDuplicate&&(studio.type==='art'));\n  const studiosMusic = [...studios].filter((studio)=>!studio.isDuplicate&&(studio.type==='music'));\n  const studiosSport = [...studios].filter((studio)=>!studio.isDuplicate&&(studio.type==='sport')).reverse();\n  const studiosTeenAge = [...studios].filter((studio)=>!studio.isDuplicate&&(studio.age_min>=12&&studio.age_min<=16));\n  const studiosParents = [...studios].filter((studio)=>!studio.isDuplicate&&(studio.age_min>=18&&studio.age_min<55)); \n  \n  return (\n    <>  \n      <Navbar/>    \n      <Header title=\"Студии и секции\"/>\n      <main className=\"studios\">         \n        <div className=\"container\">  \n\n        <Compilation studios={studiosKids} title={'Для самых маленьких (до 6 лет):'} visibleProps={true}/>\n        <Compilation studios={studiosMusic} title={'Музыка:'}/>\n        <Compilation studios={studiosDance} title={'Танцы:'}/>\n        <Compilation studios={studiosSport} title={'Спорт:'}/>\n        <Compilation studios={studiosArt} title={'Порисовать:'}/>\n        <Compilation studios={studiosTeenAge} title={'Тинэйджерам:'}/>\n        <Compilation studios={studiosParents} title={'Родителям:'}/>\n\n        <h2>ВСЕ СТУДИИ:</h2>\n\n          <ul className=\"studios__list\">\n            {studiosCopy&&studiosCopy.slice(0,showingCards).map((card)=><li key={card.title + card._id} className=\"studios__item\">\n              <Card {...card}/>\n            </li>)}            \n          </ul>\n        </div>\n      {showingCards<=studiosCopy.length &&\n      <button className=\"btn events__btn\" onClick={handleClickShowMore}>Показать еще</button>\n      }       \n      </main>\n      \n    </>\n  );\n};\n\nexport default Studios;\n",["199","200"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events.jsx",[],["201","202"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/data/events-mock.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/schedule.jsx",["203","204","205"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events-slider.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/App.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/navbar.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/home.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/events.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/data/studios-mock.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/detail-page.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/footer.jsx",[],["206","207"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/contacts.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/event.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/events-popup.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/about.jsx",[],["208","209"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/pages/documents.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/index.js",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/events-month.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/header.jsx",["210"],"import React from 'react';\nimport {useHistory} from 'react-router-dom