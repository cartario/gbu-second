ards, setTodayCards] = React.useState(null);\n  const dayStatus = sayWelcome();\n\n  React.useEffect(() => {\n    //TODO fetch cards\n    const todayCards = studios.filter((card) => {\n      if (!card.day) {\n        return null;\n      }\n      return card.day.includes(day);\n    });\n\n    setTodayCards(todayCards);\n  }, []);\n\n  if (!todayCards) {\n    return null;\n  }\n\n  const addreses = [...new Set(todayCards.map((card) => card.adress).sort(() => -1))];\n  //todo replace to reduce, get Lusin of the list by first priority\n\n  return (\n    <section className=\"now\">\n      <div>\n        {day === HOLIDAY ? (\n          <h3 className=\"now__holiday\">\n            Сегодня выходной и занятия закончилиь. Однако до понедельника вы можете присмотреть себе{' '}\n            <a href=\"/studios\">студию/секцию</a> по душе и записаться онлайн\n          </h3>\n        ) : (\n          ''\n        )}\n      </div>\n      {todayCards.length && dayStatus.status !== 0 ? (\n        <div className=\"sessions\">\n          <h3>Занятия на сегодня:</h3>\n          {addreses.map((addres) => (\n            <SessionTodayAddress\n              key={addres}\n              dataList={todayCards.filter((card) => card.adress === addres)}              \n            />\n          ))}\n        </div>\n      ) : (\n        ''\n      )}\n    </section>\n  );\n};\n\nexport default SessionsTodayBoard;\n","/Users/vasiliy/Desktop/academy/gbu-second/client/src/utils.js",[],["364","365"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/soon-events-categories.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/sessions-today-addres.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/components/studios-compilation.jsx",[],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/hooks/http.hook.js",["366"],"/Users/vasiliy/Desktop/academy/gbu-second/client/src/context/auth.context.js",[],["367","368"],"/Users/vasiliy/Desktop/academy/gbu-second/client/sr